#include <stdio.h>
#include <locale.h>

int main (){
	
	setlocale(LC_ALL, "Portuguese");

    float C, F;

    printf("BEM-VINDO AO CONVERSOR DE TEMPERATURA!\n");
    printf("\nDigite a temperatura em Celsius.\n");
    scanf("%f", &C);

    F =(9.0/5.0) * C + 32;

    printf("\n%.2f Celsius é igual a %.2f Fahrenheit.\n", C, F);
    printf("\nO conversor será finalizado.\n");

    system("pause");
    return 0;
}